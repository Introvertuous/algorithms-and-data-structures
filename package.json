{
  "name": "algorithms_and_data_structures",
  "version": "0.0.1",
  "description": "Algorithms and data structures in typescript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Introvertuous/algorithms_and_data_structures"
  },
  "scripts": {
    "precommit": "lint-staged",
    "dev": "tsc --watch",
    "build": "tsc",
    "dev-test": "jest dist --watch",
    "lint": "tslint src/*/**.ts",
    "test": "yarn lint && jest dist"
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.21",
    "husky": "^0.14.3",
    "jest-cli": "^20.0.0",
    "lint-staged": "^5.0.0",
    "prettier": "^1.7.4",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.3.2"
  },
  "jest": {
    "verbose": true
  },
  "lint-staged": {
    "*.{ts,js,json,css}": ["prettier --write", "git add"]
  }
}
